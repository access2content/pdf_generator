{
  "name": "pdf-generation",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "PDF Generation",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0"
  },
  "build": {
    "appId": "com.teem.pdfgenerator",
    "productName": "PDF Generator",
    "files": [
      "**/*",
      "!node_modules/*/{CHANGELOG.md,README.md}",
      "!node_modules/*/{test,__tests__}"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
